<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.0.0-alpha1</Version>
    <Authors>CloudNimble</Authors>
    <Company>CloudNimble</Company>
    <Copyright>Copyright &amp;copy; 2017 CloudNimble, Inc.</Copyright>
    <PackageIconUrl>https://cdn.advancedrei.com/storage/nuget/breakdance.png</PackageIconUrl>
    <IncludeSource>True</IncludeSource>
    <IncludeSymbols>True</IncludeSymbols>
    <AssemblyName>CloudNimble.Breakdance.OData</AssemblyName>
    <RootNamespace>CloudNimble.Breakdance.OData</RootNamespace>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <Product>Breakdance for OData</Product>
    <PackageId>Breakdance.OData</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
